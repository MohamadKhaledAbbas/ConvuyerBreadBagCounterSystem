    "    \"# â•‘      session.run(r\\\"D:\\\\Recordings\\\\2026_02_05\\\\2026_02_09\\\\mp4\\\")               â•‘\\n\",\n",
    "    \"# â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"\\n\",\n",
    "    \"# === Dependencies (uncomment once if not installed) ===========================\\n\",\n",
    "    \"# !pip install opencv-python-headless ipywidgets Pillow tqdm pyyaml --quiet\\n\",\n",
    "    \"\\n\",\n",
    "    \"from __future__ import annotations\\n\",\n",
    "    \"import copy, csv, io, json, logging, random, shutil, traceback, uuid\\n\",\n",
    "    \"from dataclasses import dataclass, field, asdict\\n\",\n",
    "    \"from datetime import datetime\\n\",\n",
    "    \"from enum import Enum, auto\\n\",\n",
    "    \"from pathlib import Path\\n\",\n",
    "    \"from typing import Any, Callable, Dict, List, Optional, Sequence, Tuple\\n\",\n",
    "    \"\\n\",\n",
    "    \"import cv2\\n\",\n",
    "    \"import numpy as np\\n\",\n",
    "    \"from PIL import Image\\n\",\n",
    "    \"\\n\",\n",
    "    \"try:\\n\",\n",
    "    \"    import ipywidgets as widgets\\n\",\n",
    "    \"    from IPython.display import display, clear_output, HTML\\n\",\n",
    "    \"except ImportError:\\n\",\n",
    "    \"    raise ImportError(\\\"ipywidgets required: pip install ipywidgets\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"try:\\n\",\n",
    "    \"    from tqdm.notebook import tqdm\\n\",\n",
    "    \"except ImportError:\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        from tqdm import tqdm\\n\",\n",
    "    \"    except ImportError:\\n\",\n",
    "    \"        tqdm = lambda x, **kw: x  # noqa\\n\",\n",
    "    \"\\n\",\n",
    "    \"logging.basicConfig(level=logging.INFO, format=\\\"%(asctime)s [%(levelname)s] %(name)s: %(message)s\\\")\\n\",\n",
    "    \"_log = logging.getLogger(\\\"annotation_pipeline\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"# 1. DATA MODELS\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•ï¿½ï¿½ï¿½â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"\\n\",\n",
    "    \"class ReviewStatus(Enum):\\n\",\n",
    "    \"    PENDING   = auto()\\n\",\n",
    "    \"    APPROVED  = auto()\\n\",\n",
    "    \"    CORRECTED = auto()\\n\",\n",
    "    \"    REJECTED  = auto()\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"@dataclass\\n\",\n",
    "    \"class BBox:\\n\",\n",
    "    \"    x1: float; y1: float; x2: float; y2: float\\n\",\n",
    "    \"    class_id: int = 0\\n\",\n",
    "    \"    class_name: str = \\\"\\\"\\n\",\n",
    "    \"    confidence: float = 1.0\\n\",\n",
    "    \"    manually_drawn: bool = False\\n\",\n",
    "    \"    uid: str = field(default_factory=lambda: uuid.uuid4().hex[:8])\\n\",\n",
    "    \"\\n\",\n",
    "    \"    @property\\n\",\n",
    "    \"    def width(self)  -> float: return abs(self.x2 - self.x1)\\n\",\n",
    "    \"    @property\\n\",\n",
    "    \"    def height(self) -> float: return abs(self.y2 - self.y1)\\n\",\n",
    "    \"    @property\\n\",\n",
    "    \"    def cx(self)     -> float: return (self.x1 + self.x2) / 2.0\\n\",\n",
    "    \"    @property\\n\",\n",
    "    \"    def cy(self)     -> float: return (self.y1 + self.y2) / 2.0\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def to_yolo(self, img_w: int, img_h: int) -> str:\\n\",\n",
    "    \"        return f\\\"{self.class_id} {self.cx/img_w:.6f} {self.cy/img_h:.6f} {self.width/img_w:.6f} {self.height/img_h:.6f}\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"    @staticmethod\\n\",\n",
    "    \"    def from_yolo(line: str, img_w: int, img_h: int) -> \\\"BBox\\\":\\n\",\n",
    "    \"        p = line.strip().split()\\n\",\n",
    "    \"        cid, cx, cy, w, h = int(p[0]), float(p[1]), float(p[2]), float(p[3]), float(p[4])\\n\",\n",
    "    \"        return BBox(x1=(cx-w/2)*img_w, y1=(cy-h/2)*img_h, x2=(cx+w/2)*img_w, y2=(cy+h/2)*img_h, class_id=cid)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def clamp(self, img_w: int, img_h: int) -> \\\"BBox\\\":\\n\",\n",
    "    \"        return BBox(\\n\",\n",
    "    \"            x1=max(0, min(self.x1, img_w)), y1=max(0, min(self.y1, img_h)),\\n\",\n",
    "    \"            x2=max(0, min(self.x2, img_w)), y2=max(0, min(self.y2, img_h)),\\n\",\n",
    "    \"            class_id=self.class_id, class_name=self.class_name,\\n\",\n",
    "    \"            confidence=self.confidence, manually_drawn=self.manually_drawn, uid=self.uid,\\n\",\n",
    "    \"        )\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"@dataclass\\n\",\n",
    "    \"class FrameAnnotation:\\n\",\n",
    "    \"    frame_index: int\\n\",\n",
    "    \"    source_video: str\\n\",\n",
    "    \"    image_path: Optional[str] = None\\n\",\n",
    "    \"    img_w: int = 0; img_h: int = 0\\n\",\n",
    "    \"    bboxes: List[BBox] = field(default_factory=list)\\n\",\n",
    "    \"    status: ReviewStatus = ReviewStatus.PENDING\\n\",\n",
    "    \"    reviewer_notes: str = \\\"\\\"\\n\",\n",
    "    \"    timestamp: str = field(default_factory=lambda: datetime.utcnow().isoformat())\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def to_yolo_lines(self) -> List[str]:\\n\",\n",
    "    \"        return [bb.to_yolo(self.img_w, self.img_h) for bb in self.bboxes]\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"@dataclass\\n\",\n",
    "    \"class ClassMap:\\n\",\n",
    "    \"    _id2name: Dict[int, str] = field(default_factory=dict)\\n\",\n",
    "    \"    _name2id: Dict[str, int] = field(default_factory=dict)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def add(self, cid: int, name: str):\\n\",\n",
    "    \"        self._id2name[cid] = name; self._name2id[name] = cid\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def id_to_name(self, cid: int) -> str:\\n\",\n",
    "    \"        return self._id2name.get(cid, f\\\"class_{cid}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def name_to_id(self, name: str) -> int:\\n\",\n",
    "    \"        if name not in self._name2id:\\n\",\n",
    "    \"            nid = max(self._id2name.keys(), default=-1) + 1\\n\",\n",
    "    \"            self.add(nid, name)\\n\",\n",
    "    \"        return self._name2id[name]\\n\",\n",
    "    \"\\n\",\n",
    "    \"    @property\\n\",\n",
    "    \"    def names(self) -> List[str]:  return [self._id2name[k] for k in sorted(self._id2name)]\\n\",\n",
    "    \"    @property\\n\",\n",
    "    \"    def ids(self)   -> List[int]:  return sorted(self._id2name.keys())\\n\",\n",
    "    \"    def as_yaml_dict(self) -> Dict[int, str]: return dict(sorted(self._id2name.items()))\\n\",\n",
    "    \"    def __len__(self) -> int: return len(self._id2name)\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"# 2. FRAME EXTRACTOR\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"\\n\",\n",
    "    \"class FrameExtractor:\\n\",\n",
    "    \"    def __init__(self, video_path: str | Path, stride: int = 1):\\n\",\n",
    "    \"        self.video_path = Path(video_path)\\n\",\n",
    "    \"        self.stride = max(1, stride)\\n\",\n",
    "    \"        if not self.video_path.exists():\\n\",\n",
    "    \"            raise FileNotFoundError(f\\\"Video not found: {self.video_path}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def extract(self, output_dir: Path, max_frames: Optional[int] = None) -> List[Tuple[int, Path]]:\\n\",\n",
    "    \"        output_dir.mkdir(parents=True, exist_ok=True)\\n\",\n",
    "    \"        cap = cv2.VideoCapture(str(self.video_path))\\n\",\n",
    "    \"        if not cap.isOpened():\\n\",\n",
    "    \"            raise RuntimeError(f\\\"Cannot open: {self.video_path}\\\")\\n\",\n",
    "    \"        results, idx, saved = [], 0, 0\\n\",\n",
    "    \"        try:\\n\",\n",
    "    \"            while True:\\n\",\n",
    "    \"                ret, frame = cap.read()\\n\",\n",
    "    \"                if not ret: break\\n\",\n",
    "    \"                if idx % self.stride == 0:\\n\",\n",
    "    \"                    fname = f\\\"{self.video_path.stem}_frame_{idx:06d}.jpg\\\"\\n\",\n",
    "    \"                    out = output_dir / fname\\n\",\n",
    "    \"                    cv2.imwrite(str(out), frame)\\n\",\n",
    "    \"                    results.append((idx, out)); saved += 1\\n\",\n",
    "    \"                    if max_frames and saved >= max_frames: break\\n\",\n",
    "    \"                idx += 1\\n\",\n",
    "    \"        finally:\\n\",\n",
    "    \"            cap.release()\\n\",\n",
    "    \"        _log.info(\\\"Extracted %d frames from %s (stride=%d)\\\", len(results), self.video_path.name, self.stride)\\n\",\n",
    "    \"        return results\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"# 3. PIPELINE ADAPTER\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"\\n\",\n",
    "    \"class PipelineAdapter:\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    Wraps your DetectClassifyPipeline. Override ``_parse_detections`` if\\n\",\n",
    "    \"    your pipeline returns a different shape.\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    def __init__(self, base_config: Any, pipeline_cls: type, class_map: ClassMap):\\n\",\n",
    "    \"        self._base_config = base_config\\n\",\n",
    "    \"        self._pipeline_cls = pipeline_cls\\n\",\n",
    "    \"        self.class_map = class_map\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def run_on_video(self, video_path: str | Path) -> List[FrameAnnotation]:\\n\",\n",
    "    \"        cfg = copy.deepcopy(self._base_config)\\n\",\n",
    "    \"        cfg.video_path = str(video_path)\\n\",\n",
    "    \"        pipeline = self._pipeline_cls(cfg)\\n\",\n",
    "    \"        raw = pipeline.process_video()\\n\",\n",
    "    \"        return self._parse_detections(raw, str(video_path))\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def _parse_detections(self, raw: Any, video_path: str) -> List[FrameAnnotation]:\\n\",\n",
    "    \"        \\\"\\\"\\\"\\n\",\n",
    "    \"        Default assumes raw is list of dicts:\\n\",\n",
    "    \"            [{\\\"frame_index\\\":int, \\\"img_w\\\":int, \\\"img_h\\\":int,\\n\",\n",
    "    \"              \\\"detections\\\":[{\\\"x1\\\",\\\"y1\\\",\\\"x2\\\",\\\"y2\\\",\\\"class_id\\\",\\\"class_name\\\",\\\"confidence\\\"}]}]\\n\",\n",
    "    \"        Override for your format.\\n\",\n",
    "    \"        \\\"\\\"\\\"\\n\",\n",
    "    \"        annots = []\\n\",\n",
    "    \"        if raw is None: return annots\\n\",\n",
    "    \"        items = raw if isinstance(raw, list) else raw.get(\\\"frames\\\", []) if isinstance(raw, dict) else []\\n\",\n",
    "    \"        for entry in items:\\n\",\n",
    "    \"            fa = FrameAnnotation(\\n\",\n",
    "    \"                frame_index=entry.get(\\\"frame_index\\\", 0), source_video=video_path,\\n\",\n",
    "    \"                img_w=entry.get(\\\"img_w\\\", 0), img_h=entry.get(\\\"img_h\\\", 0),\\n\",\n",
    "    \"            )\\n\",\n",
    "    \"            for det in entry.get(\\\"detections\\\", []):\\n\",\n",
    "    \"                cname = det.get(\\\"class_name\\\", \\\"\\\")\\n\",\n",
    "    \"                cid = det.get(\\\"class_id\\\", self.class_map.name_to_id(cname))\\n\",\n",
    "    \"                if cname and cid not in self.class_map._id2name:\\n\",\n",
    "    \"                    self.class_map.add(cid, cname)\\n\",\n",
    "    \"                fa.bboxes.append(BBox(\\n\",\n",
    "    \"                    x1=det[\\\"x1\\\"], y1=det[\\\"y1\\\"], x2=det[\\\"x2\\\"], y2=det[\\\"y2\\\"],\\n\",\n",
    "    \"                    class_id=cid, class_name=self.class_map.id_to_name(cid),\\n\",\n",
    "    \"                    confidence=det.get(\\\"confidence\\\", 1.0),\\n\",\n",
    "    \"                ))\\n\",\n",
    "    \"            annots.append(fa)\\n\",\n",
    "    \"        return annots\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"# 4. ANNOTATION STORE (persistence)\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"\\n\",\n",
    "    \"class AnnotationStore:\\n\",\n",
    "    \"    def __init__(self, save_path: Path):\\n\",\n",
    "    \"        self.save_path = save_path\\n\",\n",
    "    \"        self._annotations: List[FrameAnnotation] = []\\n\",\n",
    "    \"        self._dirty = False\\n\",\n",
    "    \"\\n\",\n",
    "    \"    @property\\n\",\n",
    "    \"    def annotations(self) -> List[FrameAnnotation]: return self._annotations\\n\",\n",
    "    \"    def add(self, fa: FrameAnnotation):    self._annotations.append(fa); self._dirty = True\\n\",\n",
    "    \"    def extend(self, fas):                 self._annotations.extend(fas); self._dirty = True\\n\",\n",
    "    \"    @property\\n\",\n",
    "    \"    def pending_count(self)  -> int: return sum(1 for a in self._annotations if a.status == ReviewStatus.PENDING)\\n\",\n",
    "    \"    @property\\n\",\n",
    "    \"    def reviewed_count(self) -> int: return sum(1 for a in self._annotations if a.status != ReviewStatus.PENDING)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def save(self, class_map: ClassMap):\\n\",\n",
    "    \"        data = {\\n\",\n",
    "    \"            \\\"meta\\\": {\\\"saved_at\\\": datetime.utcnow().isoformat(), \\\"total_frames\\\": len(self._annotations)},\\n\",\n",
    "    \"            \\\"class_map\\\": class_map.as_yaml_dict(),\\n\",\n",
    "    \"            \\\"annotations\\\": [{\\n\",\n",
    "    \"                \\\"frame_index\\\": fa.frame_index, \\\"source_video\\\": fa.source_video,\\n\",\n",
    "    \"                \\\"image_path\\\": fa.image_path, \\\"img_w\\\": fa.img_w, \\\"img_h\\\": fa.img_h,\\n\",\n",
    "    \"                \\\"status\\\": fa.status.name, \\\"reviewer_notes\\\": fa.reviewer_notes,\\n\",\n",
    "    \"                \\\"timestamp\\\": fa.timestamp, \\\"bboxes\\\": [asdict(bb) for bb in fa.bboxes],\\n\",\n",
    "    \"            } for fa in self._annotations],\\n\",\n",
    "    \"        }\\n\",\n",
    "    \"        self.save_path.parent.mkdir(parents=True, exist_ok=True)\\n\",\n",
    "    \"        with open(self.save_path, \\\"w\\\", encoding=\\\"utf-8\\\") as f:\\n\",\n",
    "    \"            json.dump(data, f, indent=2, ensure_ascii=False)\\n\",\n",
    "    \"        self._dirty = False\\n\",\n",
    "    \"        _log.info(\\\"Saved %d annotations â†’ %s\\\", len(self._annotations), self.save_path)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def load(self) -> ClassMap:\\n\",\n",
    "    \"        if not self.save_path.exists(): raise FileNotFoundError(self.save_path)\\n\",\n",
    "    \"        with open(self.save_path, encoding=\\\"utf-8\\\") as f: data = json.load(f)\\n\",\n",
    "    \"        cmap = ClassMap()\\n\",\n",
    "    \"        for k, v in data.get(\\\"class_map\\\", {}).items(): cmap.add(int(k), v)\\n\",\n",
    "    \"        self._annotations.clear()\\n\",\n",
    "    \"        for e in data.get(\\\"annotations\\\", []):\\n\",\n",
    "    \"            bboxes = [BBox(**bb) for bb in e.get(\\\"bboxes\\\", [])]\\n\",\n",
    "    \"            self._annotations.append(FrameAnnotation(\\n\",\n",
    "    \"                frame_index=e[\\\"frame_index\\\"], source_video=e[\\\"source_video\\\"],\\n\",\n",
    "    \"                image_path=e.get(\\\"image_path\\\"), img_w=e.get(\\\"img_w\\\", 0), img_h=e.get(\\\"img_h\\\", 0),\\n\",\n",
    "    \"                bboxes=bboxes, status=ReviewStatus[e.get(\\\"status\\\", \\\"PENDING\\\")],\\n\",\n",
    "    \"                reviewer_notes=e.get(\\\"reviewer_notes\\\", \\\"\\\"), timestamp=e.get(\\\"timestamp\\\", \\\"\\\"),\\n\",\n",
    "    \"            ))\\n\",\n",
    "    \"        self._dirty = False\\n\",\n",
    "    \"        _log.info(\\\"Loaded %d annotations â† %s\\\", len(self._annotations), self.save_path)\\n\",\n",
    "    \"        return cmap\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def autosave_if_dirty(self, class_map: ClassMap):\\n\",\n",
    "    \"        if self._dirty: self.save(class_map)\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"# 5. RENDERING\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"\\n\",\n",
    "    \"_PALETTE = [\\n\",\n",
    "    \"    (255,0,0),(0,255,0),(0,0,255),(255,255,0),(255,0,255),(0,255,255),\\n\",\n",
    "    \"    (128,0,0),(0,128,0),(0,0,128),(128,128,0),(128,0,128),(0,128,128),\\n\",\n",
    "    \"    (255,128,0),(255,0,128),(128,255,0),(0,255,128),(0,128,255),(128,0,255),\\n\",\n",
    "    \"    (64,64,64),(192,192,192),\\n\",\n",
    "    \"]\\n\",\n",
    "    \"\\n\",\n",
    "    \"def _color(cid: int): return _PALETTE[cid % len(_PALETTE)]\\n\",\n",
    "    \"\\n\",\n",
    "    \"def render_frame(image: np.ndarray, bboxes: List[BBox], cmap: ClassMap,\\n\",\n",
    "    \"                 sel_uid: Optional[str] = None, thick: int = 2, fscale: float = 0.5) -> np.ndarray:\\n\",\n",
    "    \"    vis = image.copy()\\n\",\n",
    "    \"    for bb in bboxes:\\n\",\n",
    "    \"        c = _color(bb.class_id)\\n\",\n",
    "    \"        t = thick + 2 if bb.uid == sel_uid else thick\\n\",\n",
    "    \"        p1, p2 = (int(bb.x1), int(bb.y1)), (int(bb.x2), int(bb.y2))\\n\",\n",
    "    \"        cv2.rectangle(vis, p1, p2, c, t)\\n\",\n",
    "    \"        lbl = f\\\"{cmap.id_to_name(bb.class_id)} {bb.confidence:.2f}\\\"\\n\",\n",
    "    \"        if bb.manually_drawn: lbl += \\\" [M]\\\"\\n\",\n",
    "    \"        (tw, th), _ = cv2.getTextSize(lbl, cv2.FONT_HERSHEY_SIMPLEX, fscale, 1)\\n\",\n",
    "    \"        cv2.rectangle(vis, (p1[0], p1[1]-th-6), (p1[0]+tw+4, p1[1]), c, -1)\\n\",\n",
    "    \"        cv2.putText(vis, lbl, (p1[0]+2, p1[1]-4), cv2.FONT_HERSHEY_SIMPLEX, fscale, (255,255,255), 1)\\n\",\n",
    "    \"    return vis\\n\",\n",
    "    \"\\n\",\n",
    "    \"def _np_to_widget(img: np.ndarray, fmt=\\\"jpeg\\\") -> widgets.Image:\\n\",\n",
    "    \"    rgb = cv2.cvtColor(img, cv2.COLOR_BGR2RGB)\\n\",\n",
    "    \"    buf = io.BytesIO(); Image.fromarray(rgb).save(buf, format=fmt.upper(), quality=92)\\n\",\n",
    "    \"    return widgets.Image(value=buf.getvalue(), format=fmt)\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"# 6. INTERACTIVE REVIEW WIDGET\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"\\n\",\n",
    "    \"class ReviewWidget:\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    ipywidgets UI: navigate frames, approve/reject, edit/delete/add bboxes,\\n\",\n",
    "    \"    change classes, add notes.  Auto-saves on every status change.\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    def __init__(self, store: AnnotationStore, cmap: ClassMap, frames_dir: Path, disp_w: int = 900):\\n\",\n",
    "    \"        self.store = store; self.cmap = cmap; self.frames_dir = frames_dir\\n\",\n",
    "    \"        self.disp_w = disp_w; self._idx = 0; self._sel_uid: Optional[str] = None\\n\",\n",
    "    \"        self._build(); self._refresh()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # --- build -----------------------------------------------------------------\\n\",\n",
    "    \"    def _build(self):\\n\",\n",
    "    \"        L = widgets.Layout\\n\",\n",
    "    \"        self.img_out = widgets.Output(layout=L(max_width=f\\\"{self.disp_w}px\\\"))\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # nav\\n\",\n",
    "    \"        self.btn_prev = widgets.Button(description=\\\"â—€ Prev\\\", button_style=\\\"info\\\")\\n\",\n",
    "    \"        self.btn_next = widgets.Button(description=\\\"Next â–¶\\\", button_style=\\\"info\\\")\\n\",\n",
    "    \"        self.slider   = widgets.IntSlider(min=0, max=max(0, len(self.store.annotations)-1),\\n\",\n",
    "    \"                                          description=\\\"Frame:\\\", continuous_update=False, layout=L(width=\\\"400px\\\"))\\n\",\n",
    "    \"        self.lbl_prog = widgets.HTML()\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # Jump-to-frame\\n\",\n",
    "    \"        self.txt_jump = widgets.BoundedIntText(min=1, max=max(1, len(self.store.annotations)),\\n\",\n",
    "    \"                                                value=1, description=\\\"Go to:\\\", layout=L(width=\\\"160px\\\"))\\n\",\n",
    "    \"        self.btn_jump = widgets.Button(description=\\\"Go\\\", button_style=\\\"info\\\", layout=L(width=\\\"50px\\\"))\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # status\\n\",\n",
    "    \"        self.btn_approve = widgets.Button(description=\\\"âœ… Approve\\\",       button_style=\\\"success\\\")\\n\",\n",
    "    \"        self.btn_reject  = widgets.Button(description=\\\"âŒ Reject\\\",        button_style=\\\"danger\\\")\\n\",\n",
    "    \"        self.btn_correct = widgets.Button(description=\\\"âœï¸ Mark Corrected\\\", button_style=\\\"warning\\\")\\n\",\n",
    "    \"        self.btn_pending = widgets.Button(description=\\\"â­ Next Pending\\\")\\n\",\n",
    "    \"        self.lbl_stats   = widgets.HTML()\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # bbox list\\n\",\n",
    "    \"        self.bbox_sel     = widgets.Select(options=[], description=\\\"Detections:\\\", layout=L(width=\\\"100%\\\", height=\\\"140px\\\"))\\n\",\n",
    "    \"        self.btn_del_bbox = widgets.Button(description=\\\"ğŸ—‘ Delete Selected\\\", button_style=\\\"danger\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # edit class\\n\",\n",
    "    \"        self.dd_class     = widgets.Dropdown(options=self.cmap.names or [\\\"<none>\\\"], description=\\\"Class:\\\")\\n\",\n",
    "    \"        self.btn_set_cls  = widgets.Button(description=\\\"Set Class\\\", button_style=\\\"primary\\\")\\n\",\n",
    "    \"        self.txt_new_cls  = widgets.Text(placeholder=\\\"New class nameâ€¦\\\", description=\\\"Add class:\\\", layout=L(width=\\\"300px\\\"))\\n\",\n",
    "    \"        self.btn_add_cls  = widgets.Button(description=\\\"+\\\", button_style=\\\"success\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # draw bbox\\n\",\n",
    "    \"        self.txt_x1 = widgets.FloatText(description=\\\"x1:\\\", layout=L(width=\\\"140px\\\"))\\n\",\n",
    "    \"        self.txt_y1 = widgets.FloatText(description=\\\"y1:\\\", layout=L(width=\\\"140px\\\"))\\n\",\n",
    "    \"        self.txt_x2 = widgets.FloatText(description=\\\"x2:\\\", layout=L(width=\\\"140px\\\"))\\n\",\n",
    "    \"        self.txt_y2 = widgets.FloatText(description=\\\"y2:\\\", layout=L(width=\\\"140px\\\"))\\n\",\n",
    "    \"        self.dd_draw_cls = widgets.Dropdown(options=self.cmap.names or [\\\"<none>\\\"], description=\\\"Class:\\\")\\n\",\n",
    "    \"        self.btn_add_bb  = widgets.Button(description=\\\"â• Add BBox\\\", button_style=\\\"success\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # notes\\n\",\n",
    "    \"        self.txt_notes = widgets.Textarea(description=\\\"Notes:\\\", placeholder=\\\"Reviewer notesâ€¦\\\",\\n\",\n",
    "    \"                                          layout=L(width=\\\"100%\\\", height=\\\"60px\\\"))\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # --- wiring ---\\n\",\n",
    "    \"        self.btn_prev.on_click(lambda _: self._go(-1))\\n\",\n",
    "    \"        self.btn_next.on_click(lambda _: self._go(1))\\n\",\n",
    "    \"        self.slider.observe(self._on_slider, names=\\\"value\\\")\\n\",\n",
    "    \"        self.btn_jump.on_click(lambda _: self._jump_to())\\n\",\n",
    "    \"        self.btn_approve.on_click(lambda _: self._set_status(ReviewStatus.APPROVED))\\n\",\n",
    "    \"        self.btn_reject.on_click(lambda _: self._set_status(ReviewStatus.REJECTED))\\n\",\n",
    "    \"        self.btn_correct.on_click(lambda _: self._set_status(ReviewStatus.CORRECTED))\\n\",\n",
    "    \"        self.btn_pending.on_click(lambda _: self._skip_pending())\\n\",\n",
    "    \"        self.btn_del_bbox.on_click(lambda _: self._del_bbox())\\n\",\n",
    "    \"        self.btn_set_cls.on_click(lambda _: self._set_cls())\\n\",\n",
    "    \"        self.btn_add_cls.on_click(lambda _: self._add_cls())\\n\",\n",
    "    \"        self.btn_add_bb.on_click(lambda _: self._add_bbox())\\n\",\n",
    "    \"        self.bbox_sel.observe(self._on_bbox_sel, names=\\\"value\\\")\\n\",\n",
    "    \"        self.txt_notes.observe(self._on_notes, names=\\\"value\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # --- layout ---\\n\",\n",
    "    \"        nav = widgets.HBox([self.btn_prev, self.slider, self.btn_next,\\n\",\n",
    "    \"                            self.txt_jump, self.btn_jump, self.btn_pending])\\n\",\n",
    "    \"        status = widgets.HBox([self.btn_approve, self.btn_reject, self.btn_correct, self.lbl_prog])\\n\",\n",
    "    \"        edit_box = widgets.VBox([\\n\",\n",
    "    \"            widgets.HTML(\\\"<b>Edit Selected Detection</b>\\\"),\\n\",\n",
    "    \"            widgets.HBox([self.dd_class, self.btn_set_cls]),\\n\",\n",
    "    \"            widgets.HBox([self.txt_new_cls, self.btn_add_cls]),\\n\",\n",
    "    \"            self.btn_del_bbox,\\n\",\n",
    "    \"        ])\\n\",\n",
    "    \"        draw_box = widgets.VBox([\\n\",\n",
    "    \"            widgets.HTML(\\\"<b>Draw New BBox (pixel coords)</b>\\\"),\\n\",\n",
    "    \"            widgets.HBox([self.txt_x1, self.txt_y1, self.txt_x2, self.txt_y2]),\\n\",\n",
    "    \"            widgets.HBox([self.dd_draw_cls, self.btn_add_bb]),\\n\",\n",
    "    \"        ])\\n\",\n",
    "    \"        sidebar = widgets.VBox([self.bbox_sel, edit_box, draw_box, self.txt_notes],\\n\",\n",
    "    \"                               layout=L(width=\\\"440px\\\"))\\n\",\n",
    "    \"        self.ui = widgets.VBox([self.lbl_stats, nav, status, widgets.HBox([self.img_out, sidebar])])\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def show(self): display(self.ui)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # --- helpers ---------------------------------------------------------------\\n\",\n",
    "    \"    def _cur(self) -> Optional[FrameAnnotation]:\\n\",\n",
    "    \"        return self.store.annotations[self._idx] if self.store.annotations else None\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def _go(self, d: int):\\n\",\n",
    "    \"        n = max(0, min(self._idx + d, len(self.store.annotations) - 1))\\n\",\n",
    "    \"        if n != self._idx: self._idx = n; self.slider.value = n; self._refresh()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def _on_slider(self, c):\\n\",\n",
    "    \"        if c[\\\"new\\\"] != self._idx: self._idx = c[\\\"new\\\"]; self._refresh()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def _jump_to(self):\\n\",\n",
    "    \"        target = self.txt_jump.value - 1  # 1-based â†’ 0-based\\n\",\n",
    "    \"        target = max(0, min(target, len(self.store.annotations) - 1))\\n\",\n",
    "    \"        if target != self._idx: self._idx = target; self.slider.value = target; self._refresh()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def _skip_pending(self):\\n\",\n",
    "    \"        n = len(self.store.annotations)\\n\",\n",
    "    \"        for off in range(1, n):\\n\",\n",
    "    \"            i = (self._idx + off) % n\\n\",\n",
    "    \"            if self.store.annotations[i].status == ReviewStatus.PENDING:\\n\",\n",
    "    \"                self._idx = i; self.slider.value = i; self._refresh(); return\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def _set_status(self, s: ReviewStatus):\\n\",\n",
    "    \"        fa = self._cur()\\n\",\n",
    "    \"        if fa is None: return\\n\",\n",
    "    \"        fa.status = s; fa.timestamp = datetime.utcnow().isoformat()\\n\",\n",
    "    \"        self.store._dirty = True\\n\",\n",
    "    \"        self.store.autosave_if_dirty(self.cmap)\\n\",\n",
    "    \"        if s in (ReviewStatus.APPROVED, ReviewStatus.REJECTED): self._go(1)\\n\",\n",
    "    \"        else: self._refresh()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # --- bbox ops --------------------------------------------------------------\\n\",\n",
    "    \"    def _on_bbox_sel(self, c): self._sel_uid = c[\\\"new\\\"]; self._refresh()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def _del_bbox(self):\\n\",\n",
    "    \"        fa = self._cur()\\n\",\n",
    "    \"        if fa and self._sel_uid:\\n\",\n",
    "    \"            fa.bboxes = [b for b in fa.bboxes if b.uid != self._sel_uid]\\n\",\n",
    "    \"            self._sel_uid = None; self.store._dirty = True; self._refresh()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def _set_cls(self):\\n\",\n",
    "    \"        fa = self._cur()\\n\",\n",
    "    \"        if not (fa and self._sel_uid): return\\n\",\n",
    "    \"        nm = self.dd_class.value; cid = self.cmap.name_to_id(nm)\\n\",\n",
    "    \"        for b in fa.bboxes:\\n\",\n",
    "    \"            if b.uid == self._sel_uid: b.class_id = cid; b.class_name = nm; break\\n\",\n",
    "    \"        if fa.status == ReviewStatus.PENDING: fa.status = ReviewStatus.CORRECTED\\n\",\n",
    "    \"        self.store._dirty = True; self._refresh()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def _add_cls(self):\\n\",\n",
    "    \"        nm = self.txt_new_cls.value.strip()\\n\",\n",
    "    \"        if not nm: return\\n\",\n",
    "    \"        self.cmap.name_to_id(nm); self.txt_new_cls.value = \\\"\\\"; self._refresh()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def _add_bbox(self):\\n\",\n",
    "    \"        fa = self._cur()\\n\",\n",
    "    \"        if fa is None: return\\n\",\n",
    "    \"        nm = self.dd_draw_cls.value; cid = self.cmap.name_to_id(nm)\\n\",\n",
    "    \"        bb = BBox(x1=self.txt_x1.value, y1=self.txt_y1.value, x2=self.txt_x2.value, y2=self.txt_y2.value,\\n\",\n",
    "    \"                  class_id=cid, class_name=nm, confidence=1.0, manually_drawn=True)\\n\",\n",
    "    \"        if fa.img_w and fa.img_h: bb = bb.clamp(fa.img_w, fa.img_h)\\n\",\n",
    "    \"        fa.bboxes.append(bb)\\n\",\n",
    "    \"        if fa.status == ReviewStatus.PENDING: fa.status = ReviewStatus.CORRECTED\\n\",\n",
    "    \"        self.store._dirty = True; self._refresh()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def _on_notes(self, c):\\n\",\n",
    "    \"        fa = self._cur()\\n\",\n",
    "    \"        if fa: fa.reviewer_notes = c[\\\"new\\\"]; self.store._dirty = True\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # --- render ----------------------------------------------------------------\\n\",\n",
    "    \"    def _refresh(self):\\n\",\n",
    "    \"        fa = self._cur()\\n\",\n",
    "    \"        if fa is None:\\n\",\n",
    "    \"            self.lbl_stats.value = \\\"<i>No annotations loaded.</i>\\\"; return\\n\",\n",
    "    \"\\n\",\n",
    "    \"        total = len(self.store.annotations); rev = self.store.reviewed_count; pend = self.store.pending_count\\n\",\n",
    "    \"        self.lbl_stats.value = (\\n\",\n",
    "    \"            f\\\"<b>Total:</b> {total} | <b style='color:green'>Reviewed:</b> {rev} | \\\"\\n\",\n",
    "    \"            f\\\"<b style='color:orange'>Pending:</b> {pend} | <b>Status:</b> <code>{fa.status.name}</code>\\\"\\n\",\n",
    "    \"        )\\n\",\n",
    "    \"        self.lbl_prog.value = (\\n\",\n",
    "    \"            f\\\"&nbsp; Frame <b>{self._idx+1}</b>/{total} | \\\"\\n\",\n",
    "    \"            f\\\"Video: <code>{Path(fa.source_video).name}</code>\\\"\\n\",\n",
    "    \"        )\\n\",\n",
    "    \"\\n\",\n",
    "    \"        with self.img_out:\\n\",\n",
    "    \"            clear_output(wait=True)\\n\",\n",
    "    \"            if fa.image_path and Path(fa.image_path).exists():\\n\",\n",
    "    \"                img = cv2.imread(fa.image_path)\\n\",\n",
    "    \"                if img is not None:\\n\",\n",
    "    \"                    vis = render_frame(img, fa.bboxes, self.cmap, self._sel_uid)\\n\",\n",
    "    \"                    # Resize for display\\n\",\n",
    "    \"                    h, w = vis.shape[:2]\\n\",\n",
    "    \"                    if w > self.disp_w:\\n\",\n",
    "    \"                        scale = self.disp_w / w\\n\",\n",
    "    \"                        vis = cv2.resize(vis, (int(w*scale), int(h*scale)))\\n\",\n",
    "    \"                    display(_np_to_widget(vis))\\n\",\n",
    "    \"\\n\",\n",
    "    \"        opts = []\\n\",\n",
    "    \"        for b in fa.bboxes:\\n\",\n",
    "    \"            tag = \\\" [M]\\\" if b.manually_drawn else \\\"\\\"\\n\",\n",
    "    \"            opts.append((f\\\"{b.uid} | {self.cmap.id_to_name(b.class_id)} ({b.confidence:.2f}){tag}\\\", b.uid))\\n\",\n",
    "    \"        self.bbox_sel.options = opts\\n\",\n",
    "    \"        self.txt_notes.value = fa.reviewer_notes or \\\"\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"        names = self.cmap.names or [\\\"<none>\\\"]\\n\",\n",
    "    \"        self.dd_class.options = names; self.dd_draw_cls.options = names\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"# 7. YOLO EXPORTER\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"\\n\",\n",
    "    \"class YOLOExporter:\\n\",\n",
    "    \"    def __init__(self, store: AnnotationStore, cmap: ClassMap, output_dir: str | Path,\\n\",\n",
    "    \"                 val_ratio: float = 0.2,\\n\",\n",
    "    \"                 include_statuses: Optional[Sequence[ReviewStatus]] = None,\\n\",\n",
    "    \"                 copy_images: bool = True):\\n\",\n",
    "    \"        self.store = store; self.cmap = cmap; self.out = Path(output_dir)\\n\",\n",
    "    \"        self.val_ratio = val_ratio; self.copy = copy_images\\n\",\n",
    "    \"        self.statuses = set(include_statuses or [ReviewStatus.APPROVED, ReviewStatus.CORRECTED])\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def export(self) -> Path:\\n\",\n",
    "    \"        annots = [fa for fa in self.store.annotations if fa.status in self.statuses and fa.image_path]\\n\",\n",
    "    \"        if not annots:\\n\",\n",
    "    \"            raise ValueError(\\\"No approved/corrected annotations. Review frames first.\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        rng = random.Random(42); rng.shuffle(annots)\\n\",\n",
    "    \"        sp = max(1, int(len(annots) * (1 - self.val_ratio)))\\n\",\n",
    "    \"        splits = {\\\"train\\\": annots[:sp], \\\"val\\\": annots[sp:]}\\n\",\n",
    "    \"\\n\",\n",
    "    \"        stats = {\\\"train\\\": 0, \\\"val\\\": 0, \\\"boxes\\\": 0}\\n\",\n",
    "    \"        for s, items in splits.items():\\n\",\n",
    "    \"            (self.out/\\\"images\\\"/s).mkdir(parents=True, exist_ok=True)\\n\",\n",
    "    \"            (self.out/\\\"labels\\\"/s).mkdir(parents=True, exist_ok=True)\\n\",\n",
    "    \"            for fa in items:\\n\",\n",
    "    \"                src = Path(fa.image_path); dst_i = self.out/\\\"images\\\"/s/src.name\\n\",\n",
    "    \"                dst_l = self.out/\\\"labels\\\"/s/(src.stem+\\\".txt\\\")\\n\",\n",
    "    \"                if not dst_i.exists():\\n\",\n",
    "    \"                    (shutil.copy2 if self.copy else lambda a, b: b.symlink_to(a.resolve()))(src, dst_i)\\n\",\n",
    "    \"                lines = fa.to_yolo_lines()\\n\",\n",
    "    \"                dst_l.write_text(\\\"\\\\n\\\".join(lines)); stats[s] += 1; stats[\\\"boxes\\\"] += len(lines)\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # data.yaml\\n\",\n",
    "    \"        names_block = \\\"\\\\n\\\".join(f\\\"  {cid}: {nm}\\\" for cid, nm in sorted(self.cmap._id2name.items()))\\n\",\n",
    "    \"        yaml = (\\n\",\n",
    "    \"            f\\\"# Auto-generated by annotation pipeline\\\\n\\\"\\n\",\n",
    "    \"            f\\\"path: {self.out.resolve()}\\\\ntrain: images/train\\\\nval: images/val\\\\n\\\\n\\\"\\n\",\n",
    "    \"            f\\\"nc: {len(self.cmap)}\\\\nnames:\\\\n{names_block}\\\\n\\\"\\n\",\n",
    "    \"        )\\n\",\n",
    "    \"        yp = self.out / \\\"data.yaml\\\"; yp.write_text(yaml, encoding=\\\"utf-8\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # summary\\n\",\n",
    "    \"        summary = {\\\"exported_at\\\": datetime.utcnow().isoformat(), **stats, \\\"classes\\\": self.cmap.as_yaml_dict()}\\n\",\n",
    "    \"        (self.out / \\\"export_summary.json\\\").write_text(json.dumps(summary, indent=2), encoding=\\\"utf-8\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        _log.info(\\\"YOLO export â†’ %s | train=%d val=%d boxes=%d\\\", self.out, stats[\\\"train\\\"], stats[\\\"val\\\"], stats[\\\"boxes\\\"])\\n\",\n",
    "    \"        return yp\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"# 8. ANNOTATION SESSION  â€“  main entry point\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"\\n\",\n",
    "    \"class AnnotationSession:\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    >>> session = AnnotationSession(config_opened, DetectClassifyPipeline,\\n\",\n",
    "    \"    ...     class_names=[\\\"person\\\", \\\"vehicle\\\", \\\"animal\\\"], frame_stride=5)\\n\",\n",
    "    \"    >>> session.run(r\\\"D:\\\\\\\\Recordings\\\\\\\\2026_02_05\\\\\\\\2026_02_09\\\\\\\\mp4\\\")\\n\",\n",
    "    \"    >>> # ... review in widget ...\\n\",\n",
    "    \"    >>> session.export_yolo(\\\"./yolo_dataset\\\")\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"    DEFAULT_WS = Path(\\\"./annotation_workspace\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def __init__(self, base_config: Any, pipeline_cls: type,\\n\",\n",
    "    \"                 workspace: str | Path | None = None,\\n\",\n",
    "    \"                 class_names: Optional[List[str]] = None,\\n\",\n",
    "    \"                 frame_stride: int = 1,\\n\",\n",
    "    \"                 max_frames_per_video: Optional[int] = None):\\n\",\n",
    "    \"        self.workspace = Path(workspace or self.DEFAULT_WS); self.workspace.mkdir(parents=True, exist_ok=True)\\n\",\n",
    "    \"        self.cmap = ClassMap()\\n\",\n",
    "    \"        for i, nm in enumerate(class_names or []): self.cmap.add(i, nm)\\n\",\n",
    "    \"        self.store   = AnnotationStore(self.workspace / \\\"annotations.json\\\")\\n\",\n",
    "    \"        self.adapter = PipelineAdapter(base_config, pipeline_cls, self.cmap)\\n\",\n",
    "    \"        self.stride  = frame_stride; self.max_frames = max_frames_per_video\\n\",\n",
    "    \"        self._fdir   = self.workspace / \\\"frames\\\"\\n\",\n",
    "    \"        self._rw: Optional[ReviewWidget] = None\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # --- pipeline execution ---------------------------------------------------\\n\",\n",
    "    \"    def run(self, mp4_dir: str | Path, recursive: bool = False,\\n\",\n",
    "    \"            run_pipeline: bool = True, resume: bool = True):\\n\",\n",
    "    \"        mp4_dir = Path(mp4_dir)\\n\",\n",
    "    \"        if not mp4_dir.is_dir(): raise FileNotFoundError(f\\\"Not found: {mp4_dir}\\\")\\n\",\n",
    "    \"        files = sorted(mp4_dir.rglob(\\\"*.mp4\\\") if recursive else mp4_dir.glob(\\\"*.mp4\\\"))\\n\",\n",
    "    \"        files = list(files)\\n\",\n",
    "    \"        if not files: print(f\\\"No .mp4 files in {mp4_dir}\\\"); return\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # resume\\n\",\n",
    "    \"        if resume and self.store.save_path.exists():\\n\",\n",
    "    \"            loaded = self.store.load()\\n\",\n",
    "    \"            for cid in loaded.ids:\\n\",\n",
    "    \"                if cid not in self.cmap._id2name: self.cmap.add(cid, loaded.id_to_name(cid))\\n\",\n",
    "    \"            _log.info(\\\"Resumed %d annotations.\\\", len(self.store.annotations))\\n\",\n",
    "    \"\\n\",\n",
    "    \"        done = {fa.source_video for fa in self.store.annotations}\\n\",\n",
    "    \"\\n\",\n",
    "    \"        for mp4 in tqdm(files, desc=\\\"Processing videos\\\"):\\n\",\n",
    "    \"            vk = str(mp4)\\n\",\n",
    "    \"            if vk in done: _log.info(\\\"Skip (done): %s\\\", mp4.name); continue\\n\",\n",
    "    \"\\n\",\n",
    "    \"            ext = FrameExtractor(mp4, stride=self.stride)\\n\",\n",
    "    \"            frames = ext.extract(self._fdir, max_frames=self.max_frames)\\n\",\n",
    "    \"\\n\",\n",
    "    \"            pipeline_annots = {}\\n\",\n",
    "    \"            if run_pipeline:\\n\",\n",
    "    \"                try:\\n\",\n",
    "    \"                    pa = self.adapter.run_on_video(mp4)\\n\",\n",
    "    \"                    pipeline_annots = {fa.frame_index: fa for fa in pa}\\n\",\n",
    "    \"                except Exception:\\n\",\n",
    "    \"                    _log.error(\\\"Pipeline failed on %s:\\\\n%s\\\", mp4, traceback.format_exc())\\n\",\n",
    "    \"\\n\",\n",
    "    \"            for fidx, fpath in frames:\\n\",\n",
    "    \"                if fidx in pipeline_annots:\\n\",\n",
    "    \"                    fa = pipeline_annots[fidx]; fa.image_path = str(fpath)\\n\",\n",
    "    \"                    img = cv2.imread(str(fpath))\\n\",\n",
    "    \"                    if img is not None: fa.img_h, fa.img_w = img.shape[:2]\\n\",\n",
    "    \"                else:\\n\",\n",
    "    \"                    img = cv2.imread(str(fpath))\\n\",\n",
    "    \"                    h, w = (img.shape[:2]) if img is not None else (0, 0)\\n\",\n",
    "    \"                    fa = FrameAnnotation(frame_index=fidx, source_video=vk,\\n\",\n",
    "    \"                                         image_path=str(fpath), img_w=w, img_h=h)\\n\",\n",
    "    \"                self.store.add(fa)\\n\",\n",
    "    \"\\n\",\n",
    "    \"        self.store.save(self.cmap)\\n\",\n",
    "    \"        self.review()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # --- interactive review ---------------------------------------------------\\n\",\n",
    "    \"    def review(self):\\n\",\n",
    "    \"        if not self.store.annotations and self.store.save_path.exists():\\n\",\n",
    "    \"            loaded = self.store.load()\\n\",\n",
    "    \"            for cid in loaded.ids:\\n\",\n",
    "    \"                if cid not in self.cmap._id2name: self.cmap.add(cid, loaded.id_to_name(cid))\\n\",\n",
    "    \"        if not self.store.annotations: print(\\\"No annotations. Run session.run() first.\\\"); return\\n\",\n",
    "    \"        self._rw = ReviewWidget(self.store, self.cmap, self._fdir)\\n\",\n",
    "    \"        self._rw.show()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # --- persistence ----------------------------------------------------------\\n\",\n",
    "    \"    def save(self):\\n\",\n",
    "    \"        self.store.save(self.cmap); print(f\\\"âœ… Saved â†’ {self.store.save_path}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def export_yolo(self, output_dir=\\\"./yolo_dataset\\\", val_ratio=0.2, copy_images=True) -> Path:\\n\",\n",
    "    \"        yp = YOLOExporter(self.store, self.cmap, output_dir, val_ratio=val_ratio, copy_images=copy_images).export()\\n\",\n",
    "    \"        print(f\\\"âœ… YOLO dataset â†’ {Path(output_dir).resolve()}\\\\n   data.yaml â†’ {yp}\\\"); return yp\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def print_statistics(self):\\n\",\n",
    "    \"        total = len(self.store.annotations)\\n\",\n",
    "    \"        by_s = {}\\n\",\n",
    "    \"        for fa in self.store.annotations: by_s[fa.status.name] = by_s.get(fa.status.name, 0) + 1\\n\",\n",
    "    \"        tb = sum(len(fa.bboxes) for fa in self.store.annotations)\\n\",\n",
    "    \"        mb = sum(sum(1 for b in fa.bboxes if b.manually_drawn) for fa in self.store.annotations)\\n\",\n",
    "    \"        print(\\\"=\\\" * 55)\\n\",\n",
    "    \"        print(\\\"  Annotation Session Statistics\\\")\\n\",\n",
    "    \"        print(\\\"=\\\" * 55)\\n\",\n",
    "    \"        print(f\\\"  Total frames  : {total}\\\")\\n\",\n",
    "    \"        for s, c in by_s.items(): print(f\\\"    {s:12s}: {c}\\\")\\n\",\n",
    "    \"        print(f\\\"  Total boxes   : {tb}\\\")\\n\",\n",
    "    \"        print(f\\\"    Model       : {tb - mb}\\\")\\n\",\n",
    "    \"        print(f\\\"    Manual      : {mb}\\\")\\n\",\n",
    "    \"        print(f\\\"  Classes ({len(self.cmap)}):\\\")\\n\",\n",
    "    \"        for cid, nm in self.cmap.as_yaml_dict().items(): print(f\\\"    {cid}: {nm}\\\")\\n\",\n",
    "    \"        print(\\\"=\\\" * 55)\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"# âœ… READY â€” use in the next cell(s):\\n\",\n",
    "    \"# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\",\n",
    "    \"print(\\\"âœ… Interactive Annotation Pipeline loaded.\\\")\\n\",\n",
